<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TheJobOrganizationApp.View.LogInPage"
             xmlns:viewmodels="clr-namespace:TheJobOrganizationApp.ViewModels.PopUpViewModels"
             x:DataType="viewmodels:LogInViewModel"
             Title="LogInPage">
    <Grid ColumnDefinitions="400" RowDefinitions="60,60,30,60,30,60" HorizontalOptions="Center" VerticalOptions="Center">
        <Label Grid.Row="0" Grid.Column="2" HorizontalOptions="Center">
            <Label.Text>Welcome Back !</Label.Text>
        </Label>
        <Entry Grid.Row="1"></Entry>
        <Entry Grid.Row="3"></Entry>
        <Button Grid.Row="5" Text="Log In" Command="{Binding goToScheldudeCommand}"></Button>
    </Grid>
</ContentPage>